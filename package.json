{
  "name": "repng",
  "version": "2.0.0-alpha.1",
  "description": "React component to PNG converter",
  "main": "index.js",
  "bin": {
    "repng": "./bin/repng.js"
  },
  "engines": {
    "node": ">=6.0.0"
  },
  "scripts": {
    "test": "ava",
    "cli-test": "./bin/repng.js example/Alt.js -w 512 -h 512 -o example -F example/RobotoMono-Regular.woff2 -f repng",
    "dev": "./bin/repng.js example/Alt.js --dev --port 8000",
    "help": "./bin/repng.js --help"
  },
  "dependencies": {
    "babel-core": "^6.24.1",
    "babel-loader": "^6.4.1",
    "babel-plugin-transform-async-to-generator": "^6.24.1",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.4.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.24.1",
    "datauri": "^1.0.4",
    "json-loader": "^0.5.4",
    "lodash": "^4.17.4",
    "memory-fs": "^0.4.1",
    "meow": "^3.7.0",
    "nightmare": "^2.10.0",
    "ora": "^1.2.0",
    "react": "^15.5.4",
    "react-dom": "^15.5.4",
    "webpack": "^2.4.1",
    "webpack-dev-server": "^2.4.2"
  },
  "devDependencies": {
    "ava": "^0.19.1",
    "is-stream": "^1.1.0"
  },
  "keywords": [
    "react",
    "png",
    "cli"
  ],
  "author": "Brent Jackson",
  "license": "MIT",
  "ava": {
    "babel": {
      "plugins": [
        "transform-async-to-generator",
        "transform-runtime"
      ],
      "presets": [
        "env",
        "stage-0",
        "react"
      ]
    }
  }
