{
  "name": "repng",
  "version": "3.0.3",
  "description": "React component to PNG converter",
  "main": "index.js",
  "bin": {
    "repng": "./cli.js"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "scripts": {
    "test": "nyc ava",
    "cover": "nyc report --reporter html",
    "start": "./cli.js examples/Alt.js -w 512 -h 512 -d examples -f repng.png --webfont examples/RobotoMono-Regular.woff2",
    "help": "./cli.js --help"
  },
  "dependencies": {
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.26.0",
    "datauri": "^1.1.0",
    "meow": "^4.0.0",
    "ora": "^2.0.0",
    "puppeteer": "^1.3.0",
    "react": "^16.3.2",
    "react-dom": "^16.3.2",
    "read-pkg-up": "^4.0.0",
    "resolve-cwd": "^2.0.0"
  },
  "devDependencies": {
    "ava": "^0.25.0",
    "is-stream": "^1.1.0",
    "nyc": "^11.7.1"
  },
  "keywords": [
    "react",
    "png",
    "cli"
  ],
  "author": "Brent Jackson",
  "license": "MIT",
  "ava": {
    "babel": {
      "plugins": [
        "transform-runtime"
      ],
      "presets": [
        "env",
        "stage-0",
        "react"
      ]
    }
  }
}
